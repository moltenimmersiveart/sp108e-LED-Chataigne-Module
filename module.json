{
	"name": "sp108e Wifi LED control",
	"type": "TCP Client",
	"path": "Hardware",
	
	"version": "0.0.1",
	"description": "Control sp108e Wifi LED controllers ",
	"url":"https://github.com/moltenryan/",
	"downloadURL": "https://github.com/moltenryan/",

	"hasInput": true,
	"hasOutput": true, 
	"hideDefaultCommands": true,

	"parameters": 
	{
		"Pixels Per Segment":
		{
			"type":"Integer",
			"default":10,
			"min":0,
		},

		"Segments":
		{
			"type":"Integer",
			"default":1,
			"min":1,
			"max":300
		},

		"Chip Type": 
		{
			"type":"Enum",
			"options": 
			{
				"SM16703":"00",
				"TM1804":"01",
				"UCS1903":"02",
				"WS2811":"03",
				"WS2801":"04",
				"SK6812":"05",
				"LPD6803":"06",
				"LPD8806":"07",
				"APA102":"08",
				"APA105":"09",
				"DMX512":"0a",
				"TM1914":"0b",
				"TM1913":"0c",
				"P9813":"0d",
				"INK1003":"0e",
				"P943S":"0f",
				"P9411":"10",
				"P9413":"11",
				"TX1812":"12",
				"TX1813":"13",
				"GS8206":"14",
				"GS8208":"15",
				"SK9822":"16",
				"TM1814":"17",
				"SK6812_RGBW":"18",
				"P9414":"19",
				"P9412":"1a"
			}
		},

		"Color Order": 
		{
			"type":"Enum",
			"options":
			{
				"RGB":"00",
				"RBG":"01",
				"GRB":"02",
				"GBR":"03",
				"BRG":"04",
				"BGR":"05"
			}
		}
	},
	
	"defaults": {
		"Input":
		{
			"enabled":false,
			"expanded":false
		},
		"Output":
		{
			"local":false,
			"RemoteHost":"192.168.1.17",
			"RemotePort":8189,
		}
	},

	"hideDefaultParameters":
	[
		"protocol",
		"autoAdd",
		"splitArguments",
		"messageStructure",
		"firstValueIsTheName",
		"baudRate",
		"output/isConnected"
	],
	
	"scripts": [
		"sp108e.js"
	],
	
	"values": 
	{
	},
	
	"commands": 
	{
		"Set Color":
		{
			"menu":"",
			"callback":"setColor",
			"parameters":
			{
				"Color":
				{
					"type":"Color",
					"default":[1,0,0,1]
				}
			}
		},

		"Set Brigthness":
		{
			"menu":"",
			"callback":"setBrightness",
			"parameters":
			{
				"Brightness":
				{
					"type":"Integer",
					"default":100,
					"min":0,
					"max":255
				}
			}
		},

		"Set Animation":
		{
			"menu":"",
			"callback":"setAnimation",
			"parameters":
			{
				"Effec":
				{
					"type":"Enum",
					"options":
					{
						"meteor":"cd",
						"breathing":"ce",
						"wave":"d1",
						"catch up":"d4",
						"static":"d3",
						"stack":"cf",
						"flash":"d2",
						"flow":"d0"
					}
				}
			}
		},

		"Set Animation Speed":
		{
			"menu":"",
			"callback":"setAnimationSpeed",
			"parameters":
			{
				"Brightness":
				{
					"type":"Integer",
					"default":100,
					"min":0,
					"max":255
				}
			}
		},

		"Toggle Off On":
		{
			"menu":"",
			"callback":"toggleOffOn",
		},
	}
}
